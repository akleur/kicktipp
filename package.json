{
  "name": "kicktipp",
  "version": "1.1.0",
  "description": "Kicktipp",
  "private": true,
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "webpack-dev-server --content-base app/dist/ --inline --history-api-fallback",
    "start:prod": "npm run build && node app/server.bundle.js",
    "frontend:dev": "set NODE_ENV=development && gulp build:frontend",
    "frontend:prod": "set NODE_ENV=production && gulp build:frontendprod",
    "socket:prod": "set NODE_ENV=production && npm run build:socket_server",
    "build:client": "webpack --progress --color --config webpack.config.js",
    "build:server": "webpack --progress --color --config webpack.server.config.js",
    "build:socket_server": "webpack --progress --color --config webpack.socket_server.config.js",
    "build": "npm run build:client && npm run build:server"
  },
  "author": "Estelle Cortet",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.1.1",
    "mongoose": "^5.0.4",
    "mongoose-paginate": "^5.0.3",
    "mysql": "^2.15.0",
    "nodemailer": "4.4.2",
    "nodemailer-smtp-transport": "^2.7.4",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-eslatest-node6": "^1.0.1",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.6",
    "es2015": "0.0.0",
    "eslint": "^4.17.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.6.1",
    "fancy-log": "^1.3.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-babel": "^7.0.1",
    "gulp-replace-task": "^0.11.0",
    "gulp-rsync": "0.0.8",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.4",
    "plugin-error": "^1.0.1",
    "webpack": "^3.11.0"
  }
